<% form_for @menu do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :link %><br />
    <%= f.text_field :link %>
  </p>
  <p>
    <%= f.label :security_level %><br />
    <%= f.select :security_level, (1..100).collect {|n| [n,n]} %>  </p>
  <p><%= f.submit %></p>
<% end %>

